{
	"name": "Docs DEV",
	"image": "mcr.microsoft.com/devcontainers/base:bullseye",
	"features": {
		"ghcr.io/devcontainers/features/python:1": {
			"installTools": true,
			"version": "latest"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"eamodio.gitlens",
				"yzhang.markdown-all-in-one",
				"DavidAnson.vscode-markdownlint",
				"fnando.linter"
			]
		}
	}
}
